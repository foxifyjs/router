{
  "name": "@foxify/router",
  "version": "1.0.0-beta.3",
  "description": "Foxify Router",
  "homepage": "https://github.com/foxifyjs/router#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxifyjs/router.git"
  },
  "bugs": {
    "url": "https://github.com/foxifyjs/router/issues"
  },
  "funding": {
    "url": "https://opencollective.com/foxify"
  },
  "license": "MIT",
  "author": {
    "name": "Ardalan Amini",
    "email": "ardalanamini22@gmail.com",
    "url": "https://ardalanamini.com"
  },
  "keywords": [
    "foxify",
    "router",
    "nodejs",
    "http",
    "request",
    "response"
  ],
  "main": ".build/cjs/index.js",
  "types": ".build/cjs/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "npm run build -- --watch",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:hybrid": "npm run build && npm run build:esm",
    "lint": "eslint src __tests__",
    "lint:format": "prettier -w src __tests__ && eslint --fix src __tests__",
    "test": "node --expose-gc node_modules/jest/bin/jest",
    "test:coverage": "npm test -- --coverage",
    "test:ci": "npm run test:coverage -- --ci --runInBand --verbose --logHeapUsage --no-cache",
    "benchmarks": "find benchmarks -maxdepth 1 -name '*.js' -exec benchmarks/start.sh {} \\;"
  },
  "peerDependencies": {
    "@types/node": "^16",
    "@foxify/http": "^1",
    "prototyped.js": "^2"
  },
  "dependencies": {
    "escape-html": "^1.0.3",
    "fast-json-stringify": "^2.7.13"
  },
  "devDependencies": {
    "@types/node": "^16.11.32",
    "@foxify/http": "^1.0.0-beta.2",
    "@foxify/inject": "^1.1.0",
    "@types/escape-html": "^1.0.2",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "prototyped.js": "^2.0.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.4"
  },
  "workspaces": [
    "benchmarks"
  ]
}
